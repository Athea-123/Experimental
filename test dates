import pandas as pd
from pyclense.standardizer import Standardizer

print("\n--- ğŸ§ª TESTING: Standardize Dates ---")

# 1. Setup messy dates
df = pd.DataFrame({'Event_Date': ['2023-01-01', '01/02/2023', 'Jan 3, 2023', '2023.04.05']})
print("âŒ BEFORE:\n", df)

# 2. Run Cleaner
cleaner = Standardizer(df)
cleaner.standardize_dates(['Event_Date'])

# 3. Show Result
print("âœ… AFTER (Should be MM/DD/YYYY):\n", cleaner.get_dataframe())